# zerosurf backend configuration

# server settings
NODE_ENV=development
PORT=3000

# openai gpt-5-nano for nsfw detection (required)
OPENAI_API_KEY=your_openai_api_key_here

# optional: cors configuration
CORS_ORIGIN=*

# optional: rate limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# optional: legacy nsfw detection apis (not used in current implementation)
# SIGHTENGINE_USER=your_sightengine_user
# SIGHTENGINE_SECRET=your_sightengine_secret
# GOOGLE_VISION_API_KEY=your_google_vision_api_key